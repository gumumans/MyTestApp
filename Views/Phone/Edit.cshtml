@model Phone


<h2>Редактирование</h2>


<div class="row">

    <div class="col-md-6">

        <form asp-action="Edit" asp-controller="Phone" method="post">
            @await Html.PartialAsync("_CreateEditFrom", Model)
            <input asp-for="Id" type="hidden" value="@Model.Id" />
            <button type="submit" > Изменить </button>

        </form>

    </div>

</div>


